<div id="map"></div>
<script>
    var map;
    function initMap() {
        var cadiz = {lat: 36.5267181, lng: -6.28910024};

        map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: 37.6000000, lng: -4.5000000},
            zoom: 8
        });

        var marker = new google.maps.Marker({
          map: map,
          position: cadiz,
          title: '250'
        });
    }
</script>
